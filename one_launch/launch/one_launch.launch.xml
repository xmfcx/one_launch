<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="map_path" default="/home/mfc/autoware_map/nishishinjuku_autoware_map"/>
  <set_parameter name="use_sim_time" value="true"/>

  <arg name="path_this_pkg" default="$(find-pkg-share one_launch)"/>

  <!-- vehicle_info and robot_state_publisher -->
